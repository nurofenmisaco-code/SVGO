{
  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "framework": "nextjs",
  "regions": ["iad1"],
  "headers": [
    {
      "source": "/api/svgo/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Authorization, X-Authorization, Content-Type"
        }
      ]
    }
  ]
}

